{
  "name": "medical-by-fastb",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "client",
    "server",
    "shared"
  ],
  "engines": {
    "node": ">=24.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "npm run dev:client & npm run dev:server",
    "dev:client": "npm run dev --workspace=@medical/client",
    "dev:server": "npm run dev --workspace=@medical/server",
    "build": "npm run build --workspace=@medical/shared && npm run build --workspace=@medical/client && npm run build --workspace=@medical/server",
    "build:clean": "node scripts/node/build-and-clean.js",
    "build:shared": "npm run build --workspace=@medical/shared",
    "build:client": "npm run build --workspace=@medical/client",
    "build:server": "npm run build --workspace=@medical/server",
    "start": "npm run start --workspace=@medical/server",
    "start:client": "npm run start --workspace=@medical/client",
    "lint": "npm run lint --workspace=@medical/client && npm run lint --workspace=@medical/server",
    "check-db": "node scripts/node/database-utilities.js",
    "db:check": "node scripts/node/database-utilities.js check",
    "db:partners": "node scripts/node/database-utilities.js partners",
    "db:slugs": "node scripts/node/database-utilities.js slugs",
    "db:utils": "node scripts/node/database-utilities.js all",
    "db:reset": "supabase db reset",
    "db:seed": "supabase db reset",
    "db:migrate": "supabase migration up",
    "db:status": "supabase db diff",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:push": "npx supabase db push",
    "db:diff": "npx supabase db diff",
    "db:new-migration": "npx supabase migration new",
    "db:migration-list": "npx supabase migration list",
    "db:optimize-migrations": "node scripts/node/optimize-migrations.js",
    "supabase:keys": "node scripts/node/get-supabase-keys.js",
    "supabase:fix-keys": "bash scripts/shell/fix-supabase-keys.sh",
    "env:setup": "cp .env.example .env.local && echo 'âœ… Created .env.local from .env.example - Please edit it with your actual values!'",
    "env:check": "node scripts/node/check-env-production.js",
    "check:env": "node scripts/node/check-env-production.js",
    "check:env:production": "node scripts/node/check-env-production.js",
    "supabase:link": "npx supabase link",
    "analyze": "npx tsx src/analysis/cli.ts",
    "analyze:unused": "npx tsx src/analysis/cli.ts unused",
    "cleanup:init": "npx tsx src/analysis/cleanup-cli.ts automate init",
    "cleanup:check": "npx tsx src/analysis/cleanup-cli.ts automate monitor",
    "cleanup:run": "npx tsx src/analysis/cleanup-cli.ts automate schedule",
    "cleanup:cicd": "npx tsx src/analysis/cleanup-cli.ts automate cicd",
    "cleanup:start": "npx tsx src/analysis/cleanup-cli.ts automate start",
    "cleanup:stop": "npx tsx src/analysis/cleanup-cli.ts automate stop",
    "cleanup:status": "npx tsx src/analysis/cleanup-cli.ts automate status",
    "cleanup:history": "npx tsx src/analysis/cleanup-cli.ts automate history",
    "cleanup:generate": "npx tsx src/analysis/cleanup-cli.ts automate generate",
    "cleanup:dashboard": "npx tsx src/analysis/cleanup-cli.ts dashboard start",
    "cleanup:dashboard:generate": "npx tsx src/analysis/cleanup-cli.ts dashboard generate",
    "cleanup:dashboard:export": "npx tsx src/analysis/cleanup-cli.ts dashboard export",
    "cleanup:dashboard:data": "npx tsx src/analysis/cleanup-cli.ts dashboard data",
    "workflow:init": "npx tsx src/analysis/cleanup-cli.ts integrate init",
    "workflow:setup": "npx tsx src/analysis/cleanup-cli.ts integrate setup",
    "workflow:status": "npx tsx src/analysis/cleanup-cli.ts integrate status",
    "workflow:build-check": "npx tsx src/analysis/cleanup-cli.ts integrate build-check",
    "workflow:maintenance": "npx tsx src/analysis/cleanup-cli.ts integrate maintenance",
    "maintenance:cleanup": "node scripts/cleanup-maintenance.js",
    "maintenance:setup-cron": "bash scripts/setup-cleanup-cron.sh"
  },
  "devDependencies": {
    "ajv": "^8.17.1",
    "supabase": "^2.51.0",
    "terser-webpack-plugin": "^5.3.14"
  },
  "overrides": {
    "quill": "^2.0.3",
    "glob": "^10.3.10"
  },
  "dependencies": {
    "postgres": "^3.4.7"
  }
}
